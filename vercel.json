{
  "version": 2,
  "builds": [
      {
          "src": "index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/vsltype",
          "dest": "index.js"
      },
      {
          "src": "/login",
          "dest": "index.js"
      },
      {
          "src": "/signup",
          "dest": "index.js"
      },
      {
          "src": "/(.*)",
          "dest": "index.js"
      }
  ]
}